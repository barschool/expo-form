RewriteEngine On
RewriteBase /expo

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_URI} !\.(jpg|png|css|js|php)$
RewriteRule ^(.*)$ expo.php?q=$1 [L,QSA]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^.*(/assets/.*)$ %{HTTP_HOST}/$1 [L]